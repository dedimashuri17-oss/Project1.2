generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
    id          Int      @id @default(autoincrement())
  name        String
  price       Int
  category    String
  image       String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
 